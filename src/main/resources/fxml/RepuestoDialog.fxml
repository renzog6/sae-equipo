        <Label layoutX="20.0" layoutY="300.0" text="Equipo" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="300.0" />
       
        <TextField fx:id="boxCodigo" alignment="CENTER" layoutX="140.0" layoutY="55.0" prefWidth="180.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="55.0" />
